---
import {projects} from '../cv.json'
import Section from './Section.astro'
Section
---
<Section title="Mis proyectos">
    <ul class="grid grid-rows-2 ">
        {projects.map(element =>{
            return(
                <article class="m-5">
                    <h1 class="text-xl to-black"><strong>{element.name}</strong></h1>
                    <p>{element.description}</p>
                    <h2>{element.startDate}, {element.endDate}</h2>
                </article>
            )
        })}
    </ul>
</Section>